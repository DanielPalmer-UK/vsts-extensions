        "react-dom": "^15.3.2",
        "webpack": "3.10.0",
        "webpack-dev-server": "2.11.1",
        "rimraf": "2.6.2",
        "sass-loader": "6.0.6",
        "sass-resources-loader": "1.3.2",
        "style-loader": "0.20.1",
        "css-loader": "0.28.9",
        "ts-loader": "3.4.0",
        "node-sass": "4.7.2",
        "typescript": "2.7.1",
        "typings": "2.1.1",
        "uglifyjs-webpack-plugin": "1.1.8",
        "tfx-cli": "0.5.4",
        "copy-webpack-plugin": "4.3.1",
        "url-loader": "0.6.2",
